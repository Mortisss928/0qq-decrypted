-- BPSD AUTO-FIXER POUR DELTA
print("Nettoyage du script en cours...")

local success, raw = pcall(function()
    return game:HttpGet("https://raw.githubusercontent.com/Mortisss928/0qq-decrypted/refs/heads/main/0qq")
end)

if success then
    -- 1. On retire les boucles qui font freeze Roblox
    raw = raw:gsub("while true do end", "task.wait()")
    raw = raw:gsub("while true do  end", "task.wait()")
    
    -- 2. On retire la vérification de la clé
    raw = raw:gsub("if u52 ~= u53", "if false")
    raw = raw:gsub("if u52 == u53", "if true")
    
    -- 3. On force l'ouverture du menu (u345)
    raw = raw .. "\n\ntask.wait(1) pcall(function() u345() end) print('Script Bypassé !')"
    
    -- 4. On lance le tout
    local final = loadstring(raw)
    if final then
        task.spawn(final)
    else
        warn("Erreur de syntaxe dans le script original.")
    end
else
    warn("Impossible de lire le fichier GitHub.")
end
